{
    "name": "silverstripe/recipe-cms",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for fully featured page and asset content editing",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.1 || ^8",
        "silverstripe/recipe-plugin": "^1.5",
        "silverstripe/recipe-core": "~4.9.0@alpha",
        "silverstripe/admin": "~1.9.0@alpha",
        "silverstripe/asset-admin": "~1.9.0@alpha",
        "silverstripe/campaign-admin": "~1.9.0@alpha",
        "silverstripe/versioned-admin": "~1.9.0@alpha",
        "silverstripe/cms": "~4.9.0@alpha",
        "silverstripe/errorpage": "~1.8.0@stable",
        "silverstripe/reports": "~4.8.0@stable",
        "silverstripe/siteconfig": "~4.9.0@alpha",
        "silverstripe/versioned": "~1.9.0@alpha",
        "silverstripe/graphql": "~3.6.0@alpha || ~4.0.0-alpha",
        "silverstripe/session-manager": "~1.0.0@stable"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "project-files": [
            "app/src/*"
        ],
        "resources-dir": "_resources"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}