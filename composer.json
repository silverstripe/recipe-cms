{
    "name": "silverstripe/recipe-cms",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for fully featured page and asset content editing",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-plugin": "2.0.0-alpha1",
        "silverstripe/recipe-core": "5.0.0-alpha1",
        "silverstripe/admin": "2.0.0-alpha1",
        "silverstripe/asset-admin": "2.0.0-alpha1",
        "silverstripe/campaign-admin": "2.0.0-alpha1",
        "silverstripe/versioned-admin": "2.0.0-alpha1",
        "silverstripe/cms": "5.0.0-alpha1",
        "silverstripe/errorpage": "2.0.0-alpha1",
        "silverstripe/reports": "5.0.0-alpha1",
        "silverstripe/siteconfig": "5.0.0-alpha1",
        "silverstripe/versioned": "2.0.0-alpha1",
        "silverstripe/graphql": "5.0.0-alpha1",
        "silverstripe/session-manager": "2.0.0-alpha1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "project-files": [
            "app/src/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
