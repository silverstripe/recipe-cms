{
    "name": "silverstripe/recipe-cms",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for fully featured page and asset content editing",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^7.1 || ^8",
        "silverstripe/recipe-plugin": "^1.5",
        "silverstripe/recipe-core": "4.8.0-rc1",
        "silverstripe/admin": "1.8.0-rc1",
        "silverstripe/asset-admin": "1.8.0-rc1",
        "silverstripe/campaign-admin": "1.8.0-rc1",
        "silverstripe/versioned-admin": "1.8.0-rc1",
        "silverstripe/cms": "4.8.0-rc1",
        "silverstripe/errorpage": "1.8.0-rc1",
        "silverstripe/reports": "4.8.0-rc1",
        "silverstripe/siteconfig": "4.8.0-rc1",
        "silverstripe/versioned": "1.8.0-rc1",
        "silverstripe/graphql": "3.5.0-rc1"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "sminnee/phpunit-mock-objects": "^3.4.5"
    },
    "extra": {
        "project-files": [
            "app/src/*"
        ],
        "resources-dir": "_resources"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
